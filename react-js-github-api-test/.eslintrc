{
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "max-len": [
      1,
      120,
      2,
      {
        "ignoreComments": true
      }
    ],
    "react/prop-types": [
      2
    ],
    "linebreak-style": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "function-paren-newline": 0
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base",
    "prettier"
  ]
}